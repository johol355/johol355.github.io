	journal = {Journal of Neurosurgery},
	author = {Boogaarts, Hieronymus D. and Van Amerongen, Martinus J. and De Vries, Joost and Westert, Gert P. and Verbeek, André L. M. and Grotenhuis, J. André and Bartels, Ronald H. M. A.},
	month = mar,
	year = {2014},
